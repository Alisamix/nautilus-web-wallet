{
  "extends": "airbnb",


      "env": {
        "es6": true
      },
      "parser": "babel-eslint",
      "rules":  {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prop-types": 0,
        "linebreak-style": 0,
         "jsx-a11y/anchor-is-valid": [ "error", {
            "components": [ "Link" ],
            "specialLink": [ "hrefLeft", "hrefRight","Link","to" ],
            "aspects": [ "noHref", "invalidHref", "preferButton" ]
          }]
      },

}
